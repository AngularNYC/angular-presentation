{
  "name": "angular-presentation",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:ru": "ng serve --aot --i18n-file=src/i18n/messages.ru.xtb --locale=ru --i18n-format=xtb",
    "build:ru": "ng build --aot --i18n-file=src/i18n/messages.ru.xtb --locale=ru --i18n-format=xtb",
    "build": "ng build",
    "build:prod": "ng build --prod --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sw": "sw-precache --root=dist --config=sw-precache-config.js",
    "sw-serve": "ng build --prod --aot && npm run sw && cd dist && http-server --port=8080 --host=localhost --entry-file=/index.html",
    "deploy": "ng build --prod --aot && npm run sw && firebase deploy",
    "deploy:ru": "npm run build:ru && firebase deploy",
    "i18n": "ng xi18n -f=xmb --output-path src/i18n"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nycJSorg/angular-presentation.git"
  },
  "public": true,
  "dependencies": {
    "core-js": "^2.4.1",
    "firebase": "^4.13.1",
    "hammerjs": "^2.0.8",
    "highlight.js": "^9.10.0",
    "intro.js": "^2.5.0",
    "node-sass": "^4.9.2",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "rxjs": "^5.5.6",
    "vue": "^2.3.4",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "angularfire2": "^5.0.0-rc.7",
    "@angular/animations": "^5.2.6",
    "@angular/cdk": "^5.2.2",
    "@angular/cli": "^1.6.6",
    "@angular/common": "^5.2.6",
    "@angular/compiler": "^5.2.6",
    "@angular/compiler-cli": "^5.2.6",
    "@angular/core": "^5.2.6",
    "@angular/flex-layout": "^5.0.0-beta.13",
    "@angular/forms": "^5.2.6",
    "@angular/http": "^5.2.6",
    "@angular/material": "^5.2.2",
    "@angular/platform-browser": "^5.2.6",
    "@angular/platform-browser-dynamic": "^5.2.6",
    "@angular/platform-server": "^5.2.6",
    "@angular/router": "^5.2.6",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "angular-ast-viz": "^0.3.3",
    "angular-stackblitz": "0.0.0",
    "babel-generator": "^6.25.0",
    "babel-traverse": "^6.25.0",
    "babel-types": "^6.25.0",
    "babylon": "^6.17.3",
    "codelyzer": "^4.0.1",
    "eslint": "^4.2.0",
    "firebase-tools": "^3.6.1",
    "github-api": "^3.0.0",
    "gomoku-tools": "^0.1.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "plugin-typescript": "^7.0.6",
    "protractor": "~5.1.0",
    "rxjs": "^5.5.6",
    "source-map-support": "0.4.3",
    "sw-precache": "^5.2.0",
    "systemjs": "^0.20.12",
    "systemjs-builder": "^0.16.4",
    "ts-node": "~2.0.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "zone.js": "^0.8.20"
  }
}
