{
  "extends": "../../tslint.json",
  "linterOptions": {
    "exclude": "src/**/*.json"
  },
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "angular-presentation",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "angular-presentation",
      "kebab-case"
    ]
  }
}
