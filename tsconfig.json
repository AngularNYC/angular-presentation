{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "baseUrl": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "paths": {
      "jszip": [
        "../../node_modules/jszip/dist/jszip.min.js"
      ],
      "source-map-support": [
        "../../apps/angular-presentation/src/dummy.ts"
      ],
      "crypto": [
        "../../apps/angular-presentation/src/dummy.ts"
      ],
      "@angular-presentation/*": [
        "libs/*"
      ],
      "@angular-presentation/presentation": [
        "libs/presentation/src/index.ts"
      ],
      "@angular-presentation/browser": [
        "libs/browser/src/index.ts"
      ],
      "@angular-presentation/console": [
        "libs/console/src/index.ts"
      ],
      "@angular-presentation/tooltips": [
        "libs/tooltips/src/index.ts"
      ],
      "@angular-presentation/utils": [
        "libs/utils/src/index.ts"
      ]
    },
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2016",
      "dom"
    ]
  },
  "exclude": [
    "src/assets"
  ]
}
